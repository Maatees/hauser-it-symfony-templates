{% block body %}
    <div class="row">
        <div class="col-lg-12", style="text-align: center;">
            <h1>Homepage Default</h1>
            <br>
            <a href={{path("homepage_new")}}>New</a>
            <br><br>
            <a href={{path("homepage_delete")}}>Delete</a>
            <br><br>
            <a href={{path("homepage_edit")}}>Edit</a>
            <br><br>
            <a href={{path("homepage_detail")}}>Detail</a>
            <br><br>
            <br><br>

            <div>
                Email: <input type="email"> Heslo: <input type="password">
                <br><br>
                Vyber Pohlavi
                <select name="sex">
                    <option value="male">Muz</option>
                    <option value="female">Zena</option>
                    <option value="other">Ostatni</option>
                </select>
                <br>
                Nahodny vyber pohlavi:
                <input type="checkbox">
                <br><br>
                Datum Narozeni <input type="date">
                <br><br>
                Vlozit obrazek <input type="file">
                <br><br>
                <h1>Otazka 1</h1>
                Odpoved 1 <input type="radio">
                <br>
                Odpoved 2 <input type="radio">
                <br>
                Odpoved 3 <input type="radio">

                <br><br>

                0% <input type="range"> 100%

                <br><br>



                <br><br><br>

                <input type="submit">

                <br><br>

            </div>




            <a href={{"/"}}>Zpet na Homepage</a>
        </div>
    </div>
{% endblock %}

